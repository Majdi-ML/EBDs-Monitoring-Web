{% extends 'base.html.twig' %}

{% block title %}Requetessql index{% endblock %}

{% block body %}
    <div class="pagetitle">
      <h1 class="pagetitle">RequÃªtes Sql</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="{{ path('app_ooredoo') }}">Home</a></li>
          <li class="breadcrumb-item">Dashboard</li>
        </ol>
      </nav>
    </div>

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Ref</th>
                <th>Etat</th>
                <th>RefComposant</th>
                <th>RgSgSiCluster</th>
                <th>RequeteSql</th>
                <th>UsernameDbName</th>
                <th>ResultatAttenduDeLaRequete</th>
                <th>PerformAction</th>
                <th>Criticite</th>
                <th>MessageAlarme</th>
                <th>Instructions</th>
                <th>IntervalleDePolling</th>
                <th>RefService</th>
                <th>Objet</th>
                <th>MonitoredBy</th>
                <th>NomTemplate</th>
                
            </tr>
        </thead>
        <tbody>
        {% for requetessql in requetessqls %}
            <tr>
                <td>{{ requetessql.id }}</td>
                <td>{{ requetessql.ref }}</td>
                <td>{{ requetessql.etat }}</td>
                <td>{{ requetessql.refComposant }}</td>
                <td>{{ requetessql.rgSgSiCluster }}</td>
                <td>{{ requetessql.requeteSql }}</td>
                <td>{{ requetessql.usernameDbName }}</td>
                <td>{{ requetessql.resultatAttenduDeLaRequete }}</td>
                <td>{{ requetessql.performAction }}</td>
                <td>{{ requetessql.criticite }}</td>
                <td>{{ requetessql.messageAlarme }}</td>
                <td>{{ requetessql.instructions }}</td>
                <td>{{ requetessql.intervalleDePolling }}</td>
                <td>{{ requetessql.refService }}</td>
                <td>{{ requetessql.objet }}</td>
                <td>{{ requetessql.monitoredBy }}</td>
                <td>{{ requetessql.nomTemplate }}</td>
                <td>
                    <a class="btn btn-info" href="{{ path('app_requetessql_show', {'id': requetessql.id}) }}">show</a>
                </td>
                <td>    
                    <a class="btn btn-warning" href="{{ path('app_requetessql_edit', {'id': requetessql.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="18">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a class="btn btn-outline-success" href="{{ path('app_requetessql_new') }}">Create new</a>
{% endblock %}
