{% extends 'base.html.twig' %}

{% block title %}LogFiles index{% endblock %}

{% block body %}
    <h1>LogFiles index</h1>

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Ref</th>
                <th>Etat</th>
                <th>RefComposant</th>
                <th>RgSgSiCluster</th>
                <th>Logfile</th>
                <th>Localisation</th>
                <th>Description</th>
                <th>FormatLogfile</th>
                <th>Separateur</th>
                <th>IntervalleDePolling</th>
                <th>MonitoredBy</th>
                <th>FourniEnAnnexe</th>
                <th>RefService</th>
                <th>NomTemplate</th>
                <th>LogConditions</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for log_file in log_files %}
            <tr>
                <td>{{ log_file.getId() }}</td>
                <td>{{ log_file.ref }}</td>
                <td>{{ log_file.etat }}</td>
                <td>{{ log_file.refComposant }}</td>
                <td>{{ log_file.rgSgSiCluster }}</td>
                <td>{{ log_file.logfile }}</td>
                <td>{{ log_file.localisation }}</td>
                <td>{{ log_file.description }}</td>
                <td>{{ log_file.formatLogfile }}</td>
                <td>{{ log_file.separateur }}</td>
                <td>{{ log_file.intervalleDePolling }}</td>
                <td>{{ log_file.monitoredBy }}</td>
                <td>{{ log_file.fourniEnAnnexe }}</td>
                <td>{{ log_file.refService }}</td>
                <td>{{ log_file.nomTemplate }}</td>
                <td>{{ log_file.logConditions }}</td>
                <td>
                    <a href="{{ path('app_log_files_show', {'id': log_file.id}) }}">show</a>
                    <a href="{{ path('app_log_files_edit', {'id': log_file.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="17">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_log_files_new') }}">Create new</a>
{% endblock %}
